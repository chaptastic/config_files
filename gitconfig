[core]
	editor = mate -w
	whitespace = -trailing-space
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = auto
[core]
	excludesfile = ~/.gitignore
[alias]
	ci = commit -v
	st = status
	co = checkout
	sre = svn rebase
	sdc = svn dcommit
	ms = merge --squash
[merge]
	tool = opendiff
